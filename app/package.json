{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun --bun run nuxt build",
    "dev": "bun --bun run nuxt dev",
    "generate": "bun --bun run nuxt generate",
    "preview": "bun --bun run nuxt preview",
    "postinstall": "bun --bun run nuxt prepare",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "prisma migrate deploy --preview-feature",
    "migrate:status": "prisma migrate status --preview-feature",
    "migrate:resolve": "prisma migrate resolve --preview-feature",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "start:db": "docker:db && prisma:generate && prisma:deploy && seed",
    "seed": "node --loader ts-node/esm prisma/seed.ts",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira"
    }
  },
  "dependencies": {
    "@radix-icons/vue": "^1.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "nuxt": "^3.12.4",
    "radix-vue": "^1.9.5",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "vue": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.25.0",
    "@digitalroute/cz-conventional-changelog-for-jira": "^8.0.1",
    "@iconify-json/carbon": "^1.1.32",
    "@iconify-json/twemoji": "^1.1.15",
    "@nuxt/devtools": "^1.3.1",
    "@nuxt/eslint": "^0.3.12",
    "@nuxtjs/color-mode": "^3.4.1",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@pinia/nuxt": "^0.5.1",
    "@prisma/client": "^5.18.0",
    "@types/bun": "^1.1.8",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.2.0",
    "@unocss/eslint-config": "^0.60.0",
    "@unocss/nuxt": "^0.60.0",
    "@vite-pwa/nuxt": "^0.7.0",
    "@vueuse/nuxt": "^10.9.0",
    "bun": "^1.1.27",
    "commitizen": "^4.3.0",
    "consola": "^3.2.3",
    "eslint": "^9.2.0",
    "eslint-plugin-format": "^0.1.1",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "^3.11.2",
    "nuxt-auth-utils": "^0.3.2",
    "pinia": "^2.1.7",
    "prisma": "^5.18.0",
    "prisma-dbml-generator": "^0.12.0",
    "shadcn-nuxt": "^0.10.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "unocss": "^0.61.9",
    "vue-tsc": "^2.0.17",
    "zod": "^3.23.8"
  }
}
